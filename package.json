{
  "name": "vott-ct",
  "version": "2.4.1",
  "description": "CanvasTools editor for the VoTT project",
  "main": "./dist/ct.js",
  "module": "./src/ts/ct.ts",
  "scripts": {
    "prepare": "node post-install.js",
    "start": "webpack-dev-server",
    "build": "npx webpack && copyfiles -f dist/ct.js samples/shared/js/",
    "test": "jest",
    "lint": "tslint src/**/*.ts --exclude **/*.d.ts",
    "lintfix": "tslint src/**/*.ts --exclude **/*.d.ts --fix"
  },
  "author": "Konstantin Kichinsky",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoft/CanvasTools-for-VOTT.git"
  },
  "bugs": {
    "url": "https://github.com/microsoft/CanvasTools-for-VOTT/issues"
  },
  "homepage": "https://github.com/microsoft/CanvasTools-for-VOTT#readme",
  "files": [
    "/lib",
    "/dist"
  ],
  "jest": {
    "modulePathIgnorePatterns": [
      "dist/",
      "lib/"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy",
      "npyjs": "<rootDir>/mocks/npyjs.js"
    },
    "transform": {
      ".(js|jsx)": "babel-jest",
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(test|spec)\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "dependencies": {
    "konva": "^8.3.9",
    "npyjs": "0.4.0",
    "onnxruntime-web": "^1.15.1",
    "snapsvg-cjs": "0.0.6"
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@types/node": "^17.0.22",
    "babel-jest": "^29.6.1",
    "canvas": "^2.9.3",
    "copyfiles": "^2.3.0",
    "cpx": "^1.5.0",
    "css-loader": "^6.8.1",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
